{
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "printWidth": 120,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "trailingComma": "all",

    "pugUseTabs": false,
    "pugAttributeSeparator": "always",
    "pugSingleQuote": false,
    "pugWrapAttributesThreshold": 3,
    "pugClassNotation": "as-is",
    "pugBracketSpacing": true,
    "pugSortAttributesBeginning": [
        "^cols$",
        "^v-else$",
        "^v-for$",
        "^:key$",
        "^v-if$",
        "^v-else-if$",
        "^v-on$",
        "^v-bind$",
        "^ref$",
        "^v-model",
        "^name$",
        "^:?type$",
        "^:value$",
        "^v-text$",
        "^:?label$",
        "^:headers$",
        "^:items$",
        "^:?item-text$",
        "^:?item-value$",
        "^:?item-disabled$",
        "^:?placeholder$",
        "^:?src$",
        "^:?color$",
        "^:?text-color$",
        "^:?icon$",
        "^:?small$"
    ],
    "pugSortAttributesEnd": [
        "^:?hint$",
        "^:?persistent-hint$",
        "^prepend-",
        "^@click:prepend",
        "^append-",
        "^@click:append",
        "^:to$",
        "^exact$",
        "^:(?!(width|height|loading|disabled|data-))",
        "^target$",
        "^:?width$",
        "^:?height$",
        "^@click",
        "^@",
        "^:loading$",
        "^:disabled$",
        "^:?data-"
    ]
}
